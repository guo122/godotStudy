; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Object",
"class": "DoubleDictionary",
"language": "GDScript",
"path": "res://Scripts/Framework/Tools/DoubleDictionary.gd"
}, {
"base": "ScrollContainer",
"class": "PanelSwipe",
"language": "GDScript",
"path": "res://Scripts/Framework/Panel/PanelSwipe.gd"
}, {
"base": "Object",
"class": "ToolsDatetime",
"language": "GDScript",
"path": "res://Scripts/Framework/Tools/ToolsDatetime.gd"
}, {
"base": "Object",
"class": "ToolsMisc",
"language": "GDScript",
"path": "res://Scripts/Framework/Tools/ToolsMisc.gd"
} ]
_global_script_class_icons={
"DoubleDictionary": "",
"PanelSwipe": "",
"ToolsDatetime": "",
"ToolsMisc": ""
}

[application]

config/name="GDWay"
run/main_scene="res://Scenes/MainScene.tscn"
config/icon="res://icon.png"

[autoload]

GLog="*res://Scripts/System/GLog.gd"
GData="*res://Scripts/System/GData.gd"
Globals="*res://Scripts/System/Globals.gd"
PanelMgr="*res://Scripts/Framework/Panel/PanelMgr.gd"
GScore="*res://Scripts/System/GScore.gd"

[display]

window/size/width=720
window/size/height=1280
window/energy_saving/keep_screen_on=false
window/handheld/orientation="portrait"
window/ios/hide_home_indicator=false
window/stretch/mode="2d"
window/stretch/aspect="keep_width"

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

environment/default_environment="res://default_env.tres"
